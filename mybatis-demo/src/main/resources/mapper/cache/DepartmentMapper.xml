<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis.test.cache.mapper.DepartmentMapper">
<!--  <cache
    eviction="FIFO"
    size="512"
    flushInterval="600000"
    readOnly="true"
  />-->

  <select id="findAll" resultType="com.mybatis.test.cache.entity.Department">
    select * from tbl_department
  </select>

  <select id="queryById" parameterType="string" resultType="com.mybatis.test.cache.entity.Department" useCache="true">
    select * from tbl_department where id = #{id}
  </select>

</mapper>
